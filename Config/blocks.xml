	<drop event="Harvest" name="resourceScrapPolymers" count="0" tag="oreWoodHarvest"/>
	<drop event="Destroy" name="treeOakDeco" count="1"/>	
    <property name="UpwardsCount" value="3" />
	<property name="MaxDamage" value="800" />
	<property name="FilterTags" value="foutdoor,ftrees"/>
