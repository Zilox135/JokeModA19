<block name="treeOak">
	<property name="Material" value="MtreeWoodFull" />
	<property name="CustomIcon" value="OakTree"/>
	<property name="DescriptionKey" value="treeOakDesc"/>
	<property name="Class" value="ModelTree"/>
	<property name="Shape" value="DistantDecoTree"/>	
	<property name="Place" value="Door" />
	<property name="Shape" value="ModelEntity" />
	<property name="Texture" value="116" />
	<property name="Tag" value="TreeTrunk" />
	<property name="BigDecorationRadius" value="1" />
	<property name="Model" value="#@modfolder:Resources/Trees.unity3d?OakTree.prefab" />
	<property name="ParticleOnDeath" value="treeGib_burnt" />
	<property name="ModelOffset" value="0,0,0" />
	<property name="MultiBlockDim" value="1,4,1"/>
	<property name="IsPlant" value="true"/>
	<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
	<drop event="Harvest" name="resourceScrapPolymers" count="20" tag="oreWoodHarvest"/>
	<drop event="Destroy" name="treePlantedOak1m" count="1"/>	
    <property name="UpwardsCount" value="3" />
	<property name="MaxDamage" value="800" />
	<property name="FilterTags" value="foutdoor,ftrees"/>
	<property name="SortOrder1" value="a0c0"/>
