<configs>
<remove xpath="blocks/block[@name='bedroll']"/>
<remove xpath="blocks/block[@name='bed01']"/>
<remove xpath="blocks/block[@name='bed02']"/>
<remove xpath="blocks/block[@name='treeStump']"/>
<append xpath="/blocks">
	
<block name="bedroll">
	<property name="DescriptionKey" value="bedGroupDesc"/>
	<property name="Extends" value="bodyBagBlack"/>	
	<property name="Class" value="SleepingBag"/>
	<property name="IndexName" value="brBlock"/> <!-- need this to show the bigger covering area -->
	<property name="Material" value="Mplants"/>
	<property name="MaxDamage" value="100"/>
	<property name="Shape" value="ModelEntity"/>
	<property name="Model" value="Entities/Misc/body_bag_yellowPrefab"/>
	<property name="ModelOffset" value="0,.5,.5"/>
	<property name="DisplayType" value="blockMulti"/>
	<property name="MultiBlockDim" value="2,2,1"/>
	<property name="FallDamage" value=".8"/>
	<property name="Place" value="TowardsPlacerInverted"/>
	<property name="OnlySimpleRotations" value="true"/>
	<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
	<property name="Stacknumber" value="10"/>
	<property name="FuelValue" value="20"/>
	<property name="CanPickup" value="true"/>
	<property name="IsTerrainDecoration" value="true"/>
	<property name="Group" value="Basics,Building,advBuilding"/>
	<property class="RepairItems"> <property name="resourceYuccaFibers" value="20"/> </property>
	<drop event="Harvest" name="resourceYuccaFibers" count="12" tag="allHarvest"/>
	<drop event="Destroy" name="resourceYuccaFibers" count="1,3"/>
	<drop event="Fall" name="resourceYuccaFibers" count="5" prob="1" stick_chance="0"/>
	<property name="DropMeshfile" value="#Other/Items?Misc/sack_droppedPrefab.prefab"/>
	<property name="EconomicValue" value="10"/>
	<property name="EconomicBundleSize" value="10"/>
	<property name="FilterTags" value="fdecor,fother,ffurniture"/>
</block>

<block name="bed01">
	<property name="DescriptionKey" value="bedGroupDesc"/>
	<property name="Class" value="SleepingBag"/>
	<property name="Material" value="Mmetal_weak"/>
	<property name="Shape" value="ModelEntity"/>
	<property name="Path" value="solid"/>
	<property name="Model" value="Entities/Furniture/gurneyPrefab" param1="main_mesh"/>
	<property name="ModelOffset" value="0,.5,.5"/>
	<property name="IsTerrainDecoration" value="true"/>
	<property name="DisplayType" value="blockMulti"/>
	<property name="MultiBlockDim" value="1,1,2"/>
	<property name="FallDamage" value=".6"/>
	<property name="OnlySimpleRotations" value="true"/>
	<property name="Place" value="TowardsPlacerInverted"/>
	<property name="FuelValue" value="6"/>
	<property class="RepairItems"><property name="resourceCloth" value="20"/></property>
	<property name="DowngradeBlock" value="bed01Frame"/>
	<drop event="Harvest" name="resourceCloth" count="8" tag="allHarvest"/>
	<drop event="Destroy" count="0"/>
	<property name="Group" value="Decor/Miscellaneous"/>
	<property name="EconomicValue" value="1330"/>
	<property name="FilterTags" value="fdecor,fother,ffurniture"/>
</block>

<block name="bed02">
	<property name="DescriptionKey" value="bedGroupDesc"/>
	<property name="Class" value="SleepingBag"/>
	<property name="Extends" value="gurneyBed"/>
	<property name="Model" value="Entities/Furniture/hospitalBedPrefab" param1="main_mesh"/>
	<property name="Material" value="Mmetal_weak"/>
	<property name="DisplayType" value="blockMulti"/>
	<property name="MultiBlockDim" value="2,1,2"/>
	<property name="IsTerrainDecoration" value="true"/>
	<property name="FallDamage" value=".6"/>
	<property class="RepairItems">
		<property name="resourceCloth" value="40"/>
		<property name="resourceSpring" value="20"/>
		<property name="resourceWood" value="20"/>
		<property name="resourceMetalPipe" value="10"/>
	</property>
	<drop event="Harvest" name="terrStone" count="0" tool_category="Disassemble"/>
	<drop event="Harvest" name="resourceCloth" count="16" tag="allHarvest"/>
	<drop event="Harvest" name="resourceWood" count="8" tag="allHarvest"/>
	<drop event="Harvest" name="resourceSpring" count="6" tag="salvageHarvest"/>
	<drop event="Harvest" name="resourceMetalPipe" count="2,3" tag="salvageHarvest"/>
	<drop event="Destroy" name="resourceScrapIron" count="10,15"/>
	<property name="Group" value="Decor/Miscellaneous"/>
	<property name="EconomicValue" value="2600"/>
	<property name="FilterTags" value="fdecor,fother,ffurniture"/>
</block>

<block name="treeOak">
	<property name="Material" value="MtreeWoodFull" />
	<property name="CustomIcon" value="OakTree"/>
	<property name="DescriptionKey" value="treeOakDesc"/>
	<property name="Class" value="ModelTree"/>
	<property name="Shape" value="DistantDecoTree"/>	
	<property name="Place" value="Door" />
	<property name="Shape" value="ModelEntity" />
	<property name="Texture" value="116" />
	<property name="Tag" value="TreeTrunk" />
	<property name="BigDecorationRadius" value="1" />
	<property name="Model" value="#@modfolder:Resources/Trees.unity3d?OakTree.prefab" />
	<property name="ParticleOnDeath" value="treeGib_burnt" />
	<property name="ModelOffset" value="0,0,0" />
	<property name="MultiBlockDim" value="1,4,1"/>
	<property name="IsPlant" value="true"/>
	<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
	<drop event="Harvest" name="resourceScrapPolymers" count="20" tag="oreWoodHarvest"/>
	<drop event="Destroy" name="treePlantedOak1m" count="1"/>	
    <property name="UpwardsCount" value="3" />
	<property name="MaxDamage" value="800" />
	<property name="FilterTags" value="foutdoor,ftrees"/>
	<property name="SortOrder1" value="a0c0"/>
	<property name="ActivationDistance" value="12"/>
</block>

